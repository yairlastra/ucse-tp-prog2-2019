@{
    ViewBag.Title = "Cuaderno de comunicaciones";
    Contratos.UsuarioLogueado usuarioLogueado = ViewBag.User as Contratos.UsuarioLogueado;
    Contratos.Hijo[] alumnos = ViewBag.Hijos as Contratos.Hijo[];
    Contratos.Sala[] salas = ViewBag.Salas as Contratos.Sala[];
    int[] seleccionadas = ViewBag.SalasSeleccionadas as int[];

    if (seleccionadas == null)
    {
        seleccionadas = new int[] { };
    }
}

<h2>@ViewBag.Title</h2>

@if (usuarioLogueado.RolSeleccionado != Contratos.Roles.Padre)
{
    <div class="well">
        <form action="" method="get" id="filterForm">
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label">Selección de salas</label>
                        <div>
                            @foreach (var item in salas)
                            {
                                <input type="checkbox" name="salas" value="@item.Id" @(seleccionadas.Contains(item.Id) ? "checked":"") /> @item.Nombre <text>&nbsp;&nbsp;&nbsp;</text>
                            }
                        </div>
                    </div>
                </div>
            </div>            
        </form>
    </div>
}

    @if (alumnos != null && alumnos.Length > 0)
    {
        <div class="row">
            <div class="col-lg-4">
                <div class="ibox float-e-margins">
                    <div class="ibox-title">
                        <h5>@(usuarioLogueado.RolSeleccionado == Contratos.Roles.Padre ? "Hijos" : "Alumnos")</h5>
                    </div>
                    <div class="ibox-content">
                        <div class="list-group students students-vertical">
                            @foreach (var item in alumnos)
                            {
                                <div class="list-group-item">
                                    <a href="javascript:void(0)" onclick="seleccionAlumno(this, @item.Id)">
                                        <img class="img-circle circle-border" style="width:50px;height:50px" src="data:image/png;base64,">
                                        <h3 class="list-group-item-heading text-center">@(item.Apellido + ", " + @item.Nombre) <br /><em><small>@item.Sala.Nombre</small></em></h3>
                                    </a>
                                </div>
                            }
                            <div style="clear:both"></div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-8">
                <div class="ibox">
                    <div class="ibox-title">Información de la nota</div>
                    <div class="ibox-content">
                        <form action="" method="post" id="notaForm">
                            <div class="form-group">
                                <label class="control-label">Título</label>
                                <input type="text" name="Titulo" value="" class="form-control" />
                            </div>
                            <div class="form-group">
                                <label class="control-label">Descripción</label>
                                <textarea type="text" name="Descripcion" value="" class="form-control" rows="6"></textarea>
                            </div>
                            <div class="form-group">
                                <input type="submit" name="" class="btn btn-primary" value="Crear nota" id="EnviarNota" />
                            </div>                            
                        </form>
                    </div>
                </div>
            </div>
        </div>
    }

    @section scripts{
        <script>
            function seleccionAlumno(elemento, id) {
                if ($(elemento).parent().hasClass("active")) {
                    $("#notaForm #alumno_" + id).remove();
                } else {
                    $("#notaForm").append("<input type='hidden' name='alumnos' id='alumno_"+ id+"' value='"+ id +"' />");
                }

                $(elemento).parent().toggleClass("active");

            }            

            $(document).ready(function () {
                $("#filterForm :input").change(function () {
                    $("#filterForm").submit();
                })

                $("#EnviarNota").click(function () {
                    var inputs = $("#filterForm :input");
                    $.each(inputs, function (i, e) {
                        if (e.checked)
                            $("#notaForm").append("<input type='hidden' name='salas' id='sala_" + i + "' value='" + $(e).val() + "' />");
                    });
                })
            });
        </script>

    }